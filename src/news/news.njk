---
pagination:
  data: collections.news
  size: 9
  alias: posts
---

{% extends 'layouts/default.njk' %}
{% from "partials/card.njk" import card %}

{% set title = "News" %}

{% block content %}
    <div class="row">
        {% for post in collections.news %}
            {{ card(post) }}
        {% endfor %}
    </div>
{% endblock%}
